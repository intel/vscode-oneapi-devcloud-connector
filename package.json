{
  "name": "intel-corporation.oneapi-devcloud-connect",
  "displayName": "Connect to Intel oneAPI DevCloud",
  "description": "Simplifies connecting to Intel oneAPI DevCloud",
  "version": "0.0.5",
  "publisher": "intel-corporation",
  "engines": {
    "vscode": "^1.58.1"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onStartupFinished"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "configuration": [
      {
        "title": "Connect to Intel oneAPI DevCloud",
        "properties": {
          "intel-corporation.oneapi-devcloud-connect.connection_timeout": {
            "type": "number",
            "description": "%capabilities.connection_timeout.description%",
            "default": 2000
          },
          "intel-corporation.oneapi-devcloud-connect.ssh_client": {
            "type": "string",
            "description": "%capabilities.ssh_client.description%"
          },
          "intel-corporation.oneapi-devcloud-connect.ssh_config": {
            "type": "string",
            "description": "%capabilities.ssh_config.description%"
          }
        }
      }
    ],
    "commands": [
      {
        "command": "intel-corporation.oneapi-devcloud-connect.setupConnection",
        "title": "Intel DevCloud: Setup connection"
      },
      {
        "command": "intel-corporation.oneapi-devcloud-connect.closeConnection",
        "title": "Intel DevCloud: Close connection"
      },
      {
        "command": "intel-corporation.oneapi-devcloud-connect.help",
        "title": "Intel DevCloud: Get help"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "package": "vsce package --yarn",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/glob": "^7.1.4",
    "@types/mocha": "^9.0.0",
    "@types/node": "^16.4.6",
    "@types/vscode": "^1.58.1",
    "@typescript-eslint/eslint-plugin": "^4.28.5",
    "@typescript-eslint/parser": "^4.28.5",
    "eslint": "^7.31.0",
    "glob": "^7.1.6",
    "mocha": "^9.0.3",
    "typescript": "^4.3.5",
    "vscode-test": "^1.6.1"
  }
}
