{
  "name": "devcloud",
  "displayName": "DevCloud",
  "description": "Simplifies connecting to Intel oneAPI DevCloud",
  "version": "0.0.4",
  "publisher": "intel-corporation",
  "engines": {
    "vscode": "^1.56.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:devcloud.setupConnection",
    "onCommand:devcloud.closeConnection",
    "onCommand:devcloud.help"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "devcloud.setupConnection",
        "title": "Intel DevCloud: Setup connection"
      },
      {
        "command": "devcloud.closeConnection",
        "title": "Intel DevCloud: Close connection"
      },
      {
        "command": "devcloud.help",
        "title": "Intel DevCloud: Get help"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/glob": "^7.1.3",
    "@types/mocha": "^8.0.4",
    "@types/node": "^12.11.7",
    "@types/vscode": "^1.56.0",
    "@typescript-eslint/eslint-plugin": "^4.14.1",
    "@typescript-eslint/parser": "^4.14.1",
    "eslint": "^7.19.0",
    "glob": "^7.1.6",
    "mocha": "^8.2.1",
    "typescript": "^4.1.3",
    "vscode-test": "^1.5.0"
  }
}